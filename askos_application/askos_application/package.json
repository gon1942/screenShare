{
  "name": "askos-user",
  "productName": "Electron API asd",
  "version": "2.0.2",
  "homepage": "https://hamonikr.org/",
  "author": "HamoniKR Team <hamonikr-packages@googlegroups.com>",
  "description": "Electron interactive API demos",
  "license": "MIT",
  "main": "main.js",
  "bin": "cli.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "generate-test-report": "mocha --reporter=json > report.json",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "xpackage:mas": "./script/mas.sh",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks",
    "jquery": "^3.4.1",
    "cache-clean": "rm -rf ~/.electron",
    "build:linux64": "electron-builder --linux --x64"
  },
  "repository": "https://github.com/electron/electron-api-demos",
  "keywords": [
    "Electron",
    "API",
    "demoa"
  ],
  "devDependencies": {
    "@octokit/rest": "^16.43.2",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "check-for-leaks": "^1.2.1",
    "devtron": "^1.3.0",
    "electron": "^7.3.3",
    "electron-builder": "^22.3.2",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.2.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.0.2",
    "pkg": "^4.4.0",
    "request": "^2.88.2",
    "signcode": "^0.5.0",
    "spectron": "^5.0.0",
    "standard": "^8.2.0",
    "tap": "^14.11.0"
  },
  "dependencies": {
    "@exponent/electron-cookies": "^2.0.0",
    "axios": "^0.21.1",
    "dialogs": "^2.0.1",
    "electron-cookies": "^1.1.0",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.0.7",
    "electron-shortcut-normalizer": "^1.0.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "glob": "^7.1.6",
    "highlight.js": "^10.5.0",
    "mkdirp": "^1.0.4",
    "multer": "^1.4.2",
    "nodejs-system-info": "^1.0.6",
    "open": "^9.1.0",
    "os": "^0.1.2",
    "os-utils": "0.0.14",
    "process": "^0.11.10",
    "qrcode": "^1.5.1",
    "rimraf": "^3.0.2",
    "sudo-prompt": "^9.2.1",
    "systeminformation": "^4.34.9",
    "uniqid": "^5.2.0",
    "unirest": "^0.6.0",
    "update-electron-app": "^1.5.0",
    "util": "^0.12.3"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  },
  "build": {
    "icon": "./build/icons",
    "productName": "askos-user",
    "appId": "askos.user.application",
    "asar": true,
    "protocols": {
      "name": "askos-user",
      "schemes": [
        "askos-user"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "res/bin/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "res/bin/iconshm/",
      "desktop": {
        "Encoding": "UTF-8"
      }
    },
    "deb": {
      "afterInstall": "res/linux/after-install.tpl",
      "afterRemove": "res/linux/after-remove.tpl"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "dist/",
      "app": "."
    }
  }
}
